{
    "name": "JSON",
    "version": 1,
    "file_extensions": [
        "json"
    ],
    "default_text_encoding": "latin1",
    "starts_with": [
        "Object",
        "Array"
    ],
    "tokens": [
        {
            "name": "Object",
            "type": "start_stop",
            "start_regex": "\\{",
            "end_regex": "\\}",
            "multi_line": true,
            "nested_tokens": [
                "ObjectMember",
                "ValueSeparator"
            ]
        },
        {
            "name": "ObjectMember",
            "type": "group_all_children_in_same_order",
            "multi_line": true,
            "nested_tokens": [
                "Key",
                "KeySeparator",
                "Value"
            ]
        },
        {
            "name": "Key",
            "type": "start_stop",
            "start_regex": "\\\"",
            "end_regex": "\\\""
        },
        {
            "name": "Value",
            "type": "group_one_child_only",
            "must_have_one_child": true,
            "nested_tokens": [
                "Object",
                "Array",
                "String",
                "Number",
                "Boolean"
            ]
        },
        {
            "name": "Array",
            "type": "start_Stop",
            "start_regex": "\\[",
            "end_regex": "\\]",
            "multi_line": true,
            "nested_tokens": [
                "ArrayItem",
                "Separator"
            ]
        },
        {
            "name": "ArrayItem",
            "type": "group_one_child_only",
            "multi_line": true,
            "nested_tokens": [
                "Object",
                "Array",
                "String",
                "Number",
                "Boolean"
            ]
        },
        {
            "name": "String",
            "type": "start_stop",
            "start_regex": "\\\"",
            "end_regex": "\\\"",
            "nested_tokens": [
                "EscapeCharacters"
            ]
        },
        {
            "name": "Number",
            "type": "simple",
            "regex": "[-+]?[0-9]+\\.?[0-9]*"
        },
        {
            "name": "Boolean",
            "type": "simple",
            "regex": "true|false"
        },
        {
            "name": "KeySeparator",
            "type": "simple",
            "regex": ":"
        },
        {
            "name": "ValueSeparator",
            "type": "simple",
            "regex": ","
        },
        {
            "name": "EscapeCharacters",
            "type": "simple",
            "regex": "\\\\\\\\|\\\\\""
        }
    ]
}
