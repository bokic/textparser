{
  "name": "json",
  "version": 1.0,
  "caseSensitivity": false,
  "defaultFileExtensions": ["json"],
  "otherTextInside": false,
  "defaultTextEncoding": "Latin1",
  "startTokens": ["Object", "Array"],
  "tokens": {
    "Object": {
      "type": "StartStop",
      "startRegex": "{",
      "endRegex": "}",
      "textColor": "0xffd700",
      "nestedTokens": ["Key", "String", "KeyValueSeparator", "Value", "ValueSeparator"]
    },
    "Array": {
      "type": "StartStop",
      "startRegex": "\\[",
      "endRegex": "\\]",
      "textColor": "0x179fff",
      "nestedTokens": ["String", "Number", "Object", "Array", "Bool", "Null", "ValueSeparator"]
    },
    "Value": {
      "type": "GroupOneChildOnly",
      "nestedTokens": ["String", "Number", "Object", "Array", "Bool", "Null"]
    },
    "Key": {
      "type": "StartStop",
      "otherTextInside": true,
      "startRegex": "\\\"",
      "endRegex": "(\\\")\\s*:",
      "textColor": "0x9cdcfe",
      "nestedTokens": ["StringEscape"]
    },
    "String": {
      "type": "StartStop",
      "otherTextInside": true,
      "startRegex": "\\\"",
      "endRegex": "\\\"",
      "textColor": "0xce9178",
      "nestedTokens": ["StringEscape"]
    },
    "Number": {
      "type": "SimpleToken",
      "startRegex": "-?\\d+(?:\\.\\d+)?(?:e[+-]?\\d+)?",
      "textColor": "0xb5cea8"
    },
    "StringEscape": {
      "type": "SimpleToken",
      "startRegex": "\\\\\\\\|\\\\\\/|\\\\b|\\\\f|\\\\n|\\\\r|\\\\t|\\\\u[0-9a-f]{4}",
      "textColor": "0xd7ba7d"
    },
    "Bool": {
      "type": "SimpleToken",
      "startRegex": "true|false",
      "textColor": "0x088c54"
    },
    "Null": {
      "type": "SimpleToken",
      "startRegex": "null",
      "textColor": "0x0000ff"
    },
    "ValueSeparator": {
      "type": "SimpleToken",
      "startRegex": ","
    },
    "KeyValueSeparator": {
      "type": "SimpleToken",
      "startRegex": ":"
    }
  }
}
